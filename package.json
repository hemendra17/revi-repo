{
  "name": "zyrl-revi",
  "version": "1.0.0",
  "description": "Rebranding zyrl to revi",
  "main": "index.js",
  "scripts": {
    "setup": "npm install --legacy-peer-deps",
    "build": "rm dist -rf && NODE_ENV=production ./node_modules/.bin/webpack --config ./config/webpack.config.js --progress",
    "watch": "rm dist -rf && NODE_ENV=development ./node_modules/.bin/webpack --config ./config/webpack.config.js --progress --watch",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.config.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyrl-sf/revi.git"
  },
  "keywords": [
    "revi",
    "zyrl"
  ],
  "author": "Konstantin Nikitin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zyrl-sf/revi/issues"
  },
  "homepage": "https://github.com/zyrl-sf/revi",
  "dependencies": {
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.9.0",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^4.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "engines": {
    "node": ">= 15.2.1",
    "npm": ">= 7.0.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
